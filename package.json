{
  "name": "secure-bank",
  "version": "1.0.0",
  "description": "Backend for SecureBank Transactions Application",
  "main": "index.js",
  "author": {
    "name": "UbiratÃ£ Emiliano Fonseca Costa",
    "email": "ubirata.efc@gmail.com"
  },
  "license": "ISC",
  "keywords": [
    "security",
    "bank",
    "securebank",
    "transactions"
  ],
  "engines": {
    "node": ">=16.19.1",
    "yarn": ">=1.22.19"
  },
  "scripts": {
    "clean-install": "yarn install --immutable --prefer-offline --frozen-lockfile",
    "pre-push": "run-s clean-install lint style build test:run",
    "build": "tsup src",
    "start": "tsx src/index.ts",
    "start:dev": "tsx watch src/index.ts",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test": "vitest",
    "test:e2e": "NODE_ENV=test vitest run --dir test/e2e",
    "migration:create": "typeorm-ts-node-commonjs migration:create ./src/shared/db/typeorm/migrations/$npm_config_name",
    "setup:local:up": "docker-compose -f docker-compose.yaml up -d",
    "setup:local:down": "docker-compose -f docker-compose.yaml down",
    "style": "prettier . --check src/**/*.ts --cache",
    "style:fix": "prettier . --write src/**/*.ts --cache",
    "lint": "eslint . --ext .js,.ts --cache",
    "lint:fix": "eslint . --ext .js,.ts --fix --cache"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsup src && prettier --write . && eslint --fix ."
    }
  },
  "dependencies": {
    "axios": "0.21.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "1.19.0",
    "chalk": "4.0.0",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "express-validator": "6.14.2",
    "helmet": "7.0.0",
    "http-status": "1.6.2",
    "inversify": "5.1.1",
    "inversify-express-utils": "6.4.3",
    "json-stringify-safe": "5.0.1",
    "jsonwebtoken": "9.0.0",
    "luxon": "3.2.0",
    "module-alias": "2.2.2",
    "multer": "^1.4.5-lts.1",
    "otplib": "^12.0.1",
    "pg": "8.5.1",
    "qrcode": "^1.5.3",
    "reflect-metadata": "0.1.13",
    "ts-custom-error": "3.2.0",
    "typeorm": "0.3.17",
    "uuid": "^9.0.1",
    "zod": "3.20.6"
  },
  "devDependencies": {
    "@commitlint/cli": "13.2.1",
    "@commitlint/config-conventional": "13.2.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "1.7.0",
    "@types/cors": "2.8.9",
    "@types/express": "4.17.17",
    "@types/json-stringify-safe": "5.0.0",
    "@types/jsonwebtoken": "9.0.0",
    "@types/luxon": "3.2.0",
    "@types/module-alias": "2.0.1",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.0",
    "@types/qrcode": "^1.5.5",
    "@types/supertest": "2.0.12",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "5.59.7",
    "@typescript-eslint/parser": "5.59.7",
    "@vitest/coverage-istanbul": "0.31.0",
    "@vitest/ui": "0.31.0",
    "eslint": "8.41.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "3.5.5",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-import-helpers": "1.2.1",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "4.3.8",
    "node-mocks-http": "1.12.2",
    "npm-run-all": "4.1.5",
    "prettier": "2.8.8",
    "supertest": "6.3.3",
    "tsconfig-paths": "4.2.0",
    "tsup": "6.7.0",
    "tsx": "3.12.7",
    "typescript": "^5.3.2",
    "vite": "4.3.9",
    "vite-tsconfig-paths": "4.2.0",
    "vitest": "0.31.1",
    "vitest-mock-extended": "1.1.3"
  }
}
